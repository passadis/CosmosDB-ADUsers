{
  "bindings": [
    {
      "type": "cosmosDBTrigger",
      "name": "documents",
      "direction": "in",
      "leaseCollectionName": "leases",
      "connectionStringSetting": "xxxxxxx_DOCUMENTDB",
      "databaseName": "Input",
      "collectionName": "Signup",
      "createLeaseCollectionIfNotExists": true
    },
    {
      "type": "blob",
      "name": "outputBlob",
      "connection": "AzureWebJobsStorage_accountname",
      "path": "users.csv",
      "direction": "out"

    }
  ],
  "disabled": false,
  "values": {
    "AZURE_CLIENT_ID" : "xxxxxxxxxxxxxxxxxxxx",
     "AZURE_TENANT_ID" : "xxxxxxxxxxxxxxxxx",
      "AZURE_CLIENT_SECRET" : "xxxxxxxxxxxx",
    "BlobContainerName": "data",
    "BlobAccountName": "XXXXX",
    "BlobStorageConnectionString" : "xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx"
  }
}
